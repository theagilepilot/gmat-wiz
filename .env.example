# GMAT Ascension - Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI API Key (required for AI question generation)
OPENAI_API_KEY=sk-your-api-key-here

# OpenAI Model (optional, defaults to gpt-4o)
OPENAI_MODEL=gpt-4o

# Server Configuration
PORT=3000
HOST=localhost

# Database (optional, defaults to data/gmat-ascension.db)
DATABASE_PATH=data/gmat-ascension.db

# Study Documents Folder (optional, for RAG)
STUDY_DOCS_PATH=study-docs

# Development Mode
NODE_ENV=development
